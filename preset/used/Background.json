{
    "Program": "Background",
    "Preset": [
        {
            "_comment" : "키보드와 마우스 테스트",
            "drotate" : [
                {
                    "fname": "마우스 스크롤",
                    "fcodeL": "mousewheel",
                    "fcodeR": "mousewheel",
                    "execute": "forceMouse.scrollY(v)"
                },
                {
                    "fname": "확대 / 축소",
                    "fcodeL": "Control + mousewheeldown",
                    "fcodeR": "Control + mousewheelup",
                    "execute": "forceMouse.combineMouse(v, forceMouse.scrollY, 'control', ['control'])"
                },
                {
                    "fname": "키보드 X축 방향",
                    "fcodeL": "ArrowLeft",
                    "fcodeR": "ArrowRight",
                    "execute": "forceKeyboard.arrowX(v)"
                },
                {
                    "fname": "키보드 Y축 방향",
                    "fcodeL": "ArrowDown",
                    "fcodeR": "ArrowUp",
                    "execute": "forceKeyboard.arrowY(v)"
                }
            ],
            "bpush": [
                {
                    "fname": "더블 클릭",
                    "fcode": "mouseclick",
                    "execute": "forceMouse.doubleLeftClick()"
                },
                {
                    "fname": "저장",
                    "fcode": "Control + s",
                    "execute": "forceKeyboard.combineKey('s', 'control')"
                },
                {
                    "fname": "전체 선택 주석",
                    "fcode": "Control + a + k + c",
                    "execute": "forceKeyboard.combineKeys(['a', 'k', 'c'], 'control')"
                },
                {
                    "fname": "우클릭",
                    "fcode": "mouseclick",
                    "execute": "forceMouse.oneRightClick()"
                }
            ]
        },
